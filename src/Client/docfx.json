{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "build": {
    "content": [
      {
        "files": "**.{md|yml}",
        "src": "docfx-content/docs/",
        "dest": "api/"
      },
      {
        "files": "**.{md|yml}",
        "src": "docfx-content/articles/",
        "dest": "articles/"
      },
      {
        "files": "**.{md|yml}",
        "src": "docfx-content/guides/",
        "dest": "guides/"
      },
      {
        "files": "all/*.{md|yml}",
        "src": "docfx-content/api-browser/v4/",
        "dest": "api/browser/v4/",
        "group": "v4"
      },
      {
        "files": "all/*.{md|yml}",
        "src": "docfx-content/api-browser/v3/",
        "dest": "api/browser/v3/",
        "group": "v3"
      },
      {
        "files": "all/*.{md|yml}",
        "src": "docfx-content/api-browser/v2/",
        "dest": "api/browser/v2/",
        "group": "v2"
      }
    ],
    "resource": [
      {
        "files": "**.{png|jpg}",
        "src": "docfx-content/docs/",
        "dest": "api/"
      },
      {
        "files": "**.{png|jpg}",
        "src": "docfx-content/articles/",
        "dest": "articles/"
      },
      {
        "files": "**.{png|jpg}",
        "src": "docfx-content/guides/",
        "dest": "guides/"
      }
    ],
    "template": [
      "default",
      "modern",
      "docfx-content/steeltoe-template"
    ],
    "xref": [
      "wwwroot/xrefmap.yml"
    ],
    "output": "wwwroot",
    "globalMetadataFiles": [
      "metadata.json"
    ],
    "fileMetadataFiles": [],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "disableGitFeatures": false
  }
}
